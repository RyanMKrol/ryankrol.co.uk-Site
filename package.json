{
  "name": "ryankrol.co.uk",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "start-dev": "REACT_APP_LOCAL_API=1 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-ci": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/husky"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.1.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "formik": "^2.2.6",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.2",
    "@storybook/addon-essentials": "^6.2.2",
    "@storybook/addon-links": "^6.2.2",
    "@storybook/node-logger": "^6.2.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.2",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.4.1",
    "husky": "^4.2.5"
  }
}
